<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<BorderPane prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.company.electro_store.temp.MenuController">
   <left>
      <VBox fx:id="mainVbox" prefHeight="335.0" prefWidth="152.0" spacing="16.0" styleClass="style" stylesheets="@../../style/style.css" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="UserButton" alignment="BASELINE_LEFT" cache="true" depthTest="ENABLE" mnemonicParsing="false" onAction="#userButtonActive" prefHeight="45.0" prefWidth="152.0" stylesheets="@../../style/style.css" text="Профиль">
               <graphic>
                  <ImageView fitHeight="18.0" fitWidth="18.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../icons/user.png" />
                     </image>
                  </ImageView>
               </graphic>
               <styleClass>
                  <String fx:value="button3text" />
                  <String fx:value="button3" />
               </styleClass>
               <cursor>
                  <Cursor fx:constant="DEFAULT" />
               </cursor>
            </Button>
            <Button fx:id="workerButton" alignment="BASELINE_LEFT" blendMode="SCREEN" mnemonicParsing="false" onAction="#workerButtonActive" prefHeight="45.0" prefWidth="152.0" styleClass="button3" stylesheets="@../../style/style.css" text="Работники">
               <graphic>
                  <ImageView fitHeight="18.0" fitWidth="18.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../icons/workers.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Button fx:id="StorageButton" alignment="BASELINE_LEFT" mnemonicParsing="false" onAction="#storageButtonActive" prefHeight="45.0" prefWidth="200.0" styleClass="button3" stylesheets="@../../style/style.css" text="Склад">
               <graphic>
                  <ImageView fitHeight="18.0" fitWidth="18.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../icons/storage.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Button fx:id="HallButton" alignment="BASELINE_LEFT" mnemonicParsing="false" onAction="#hallButtonActive" prefHeight="45.0" prefWidth="169.0" styleClass="button3" stylesheets="@../../style/style.css" text="Зал">
               <graphic>
                  <ImageView fitHeight="18.0" fitWidth="18.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../icons/hall.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Button fx:id="GoodsButton" alignment="BASELINE_LEFT" mnemonicParsing="false" onAction="#productButtonActive" prefHeight="45.0" prefWidth="171.0" styleClass="button3" stylesheets="@../../style/style.css" text="Товары">
               <graphic>
                  <ImageView fitHeight="18.0" fitWidth="18.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../icons/goods.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Button fx:id="CountButton" alignment="BASELINE_LEFT" mnemonicParsing="false" onAction="#otherButtonActive" prefHeight="45.0" prefWidth="173.0" styleClass="button3" stylesheets="@../../style/style.css" text="Учет">
               <graphic>
                  <ImageView fitHeight="18.0" fitWidth="18.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../icons/settings.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Button alignment="BASELINE_LEFT" mnemonicParsing="false" onAction="#addReport" prefHeight="45.0" prefWidth="173.0" styleClass="button3" stylesheets="@../../style/style.css" text="Отчет">
               <graphic>
                  <ImageView fitHeight="18.0" fitWidth="18.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../icons/settings.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <HBox alignment="CENTER" fillHeight="false" prefHeight="28.0" prefWidth="152.0" spacing="10.0" style="-fx-background-color: transparent;">
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
               <children>
                  <Button mnemonicParsing="false" onAction="#closeHandler" prefHeight="30.0" prefWidth="26.0" style="-fx-background-color: rgba(238,69,38,0.5);" styleClass="button3" stylesheets="@../../style/style.css">
                     <graphic>
                        <ImageView fitHeight="34.0" fitWidth="34.0">
                           <image>
                              <Image url="@../../icons/close.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Button>
                  <Button mnemonicParsing="false" onAction="#returnHandler" prefHeight="25.0" prefWidth="50.0" styleClass="button3">
                     <graphic>
                        <ImageView fitHeight="34.0" fitWidth="34.0">
                           <image>
                              <Image url="@../../icons/logout.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Button>
               </children>
            </HBox>
         </children>
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
         <padding>
            <Insets top="30.0" />
         </padding>
      </VBox>
   </left>
   <top>
      <HBox prefHeight="53.0" prefWidth="789.0" BorderPane.alignment="CENTER">
         <children>
            <ImageView fitHeight="70.0" fitWidth="1000.0">
               <image>
                  <Image url="@../../images/BASEMENT.png" />
               </image>
            </ImageView>
         </children></HBox>
   </top>
</BorderPane>
